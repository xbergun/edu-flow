{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowJs": true,
    "types": [
      "@cap-js/cds-types",
      "@openui5/types/types",
      "@sapui5/types/types",
      "@sapui5/ts-types/types",
      "@types/uuid",
    ],
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}