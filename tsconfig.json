{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "./gen/srv/srv",
    "skipLibCheck": true,
    "sourceMap": true,
    "allowJs": true,
    "types": [
      "@cap-js/cds-types",
      "@openui5/types/types",
      "@sapui5/types/types",
      "@sapui5/ts-types/types",
      "@types/uuid"
    ],
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  },
  "include": [
    "srv"
  ],
  "exclude": [
    "node_modules"
  ]
}