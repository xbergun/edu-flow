{
  "_version": "1.65.0",
  "sap.app": {
    "id": "eduflowui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.5",
      "toolsId": "3c52b19b-1f10-4bf9-86a6-9832225cfd2d"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/edu-flow/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.133.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "eduflowui.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "defaultCountMode": "None",
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "eduflowui.view"
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": ":?query:",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "RouteStudent",
          "pattern": "student",
          "target": [
            "RouteStudent"
          ]
        },
        {
          "name": "RouteCourses",
          "pattern": "courses",
          "target": [
            "RouteCourses"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "id": "Login",
          "name": "Login"
        },
        "RouteStudent": {
          "id": "Student",
          "name": "Student"
        },
        "RouteCourses": {
          "id": "Courses",
          "name": "Courses"
        }
      }
    },
    "rootView": {
      "viewName": "eduflowui.view.App",
      "type": "XML",
      "id": "App"
    }
  }
}
